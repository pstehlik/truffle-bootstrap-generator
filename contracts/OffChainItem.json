  "source": "pragma solidity ^0.4.11;\n\nimport \"zeppelin-solidity/contracts/ownership/Ownable.sol\";\n \n/**\n * @title OffChainItem\n * @dev An that has a representation somewhere off chain. The location of the object off chain should be stored\n * in the URL format so that it is easy to find and access.\n */\ncontract OffChainItem is Ownable {\n\tstring public offChainLocation;\n\n\tfunction OffChainItem (string _offChainLocation) public {\n\t\toffChainLocation = _offChainLocation;\n\t}\n\n\tfunction getOffChainLocation() public view returns(string) { \n\t\treturn offChainLocation; \n\t}\n\n\tfunction setOffChainLocation(string _offChainLocation) onlyOwner public { \n\t\toffChainLocation = _offChainLocation;\n\t}\n\n}\n",
